#!/usr/bin/env bash

#
# Print a noticeable separator.
#
#   $ sep; ls -l
#

do1() {
  cat<<EOT

- 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 -
 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8
- 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 -

EOT
}

do2() {
  echo -e "\n\e[1;31m- 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 - 8 -\e[0m\n"
}

do3() {
  echo -e "\n\e[1;31m— ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● — ● —\e[0m\n"
}

do3
